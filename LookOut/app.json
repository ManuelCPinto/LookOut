{
  "expo": {
    "name": "LookOut",
    "slug": "LookOut",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "scheme": "lookout",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.SCMU2425.lookout",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
     "edgeToEdgeEnabled": true,
     "config": {
     "googleServicesFile": "./google-services.json"
     }    
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
          "config": { "googleServicesFile": "./google-services.json" }
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
